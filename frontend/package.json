{
  "name": "feastfrenzy",
  "version": "0.8.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npx ng build --configuration production --aot --optimization",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "e2e:ci": "start-server-and-test 'npm run start' http://localhost:4200 'npm run e2e:headless'",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:record": "cypress run --record --key $CYPRESS_RECORD_KEY",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/feastfrenzy/stats.json",
    "docker:build": "docker build -t feastfrenzy/frontend .",
    "docker:run": "docker run -p 4200:80 feastfrenzy/frontend",
    "serve:prod": "ng build --configuration production && npx http-server dist/feastfrenzy -p 4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.5",
    "@angular/common": "21.0.3",
    "@angular/compiler": "21.0.3",
    "@angular/core": "21.0.3",
    "@angular/forms": "21.0.3",
    "@angular/platform-browser": "21.0.3",
    "@angular/platform-browser-dynamic": "21.0.3",
    "@angular/router": "21.0.3",
    "bootstrap": "^5.2.3",
    "bootswatch": "^5.3.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "21.0.3",
    "@angular/cli": "21.0.3",
    "@angular/compiler-cli": "21.0.3",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "angular-eslint": "^21.1.0",
    "axe-core": "^4.10.0",
    "cypress": "^13.17.0",
    "cypress-axe": "^1.5.0",
    "eslint": "^9.39.2",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^16.0.0",
    "prettier": "^3.5.0",
    "start-server-and-test": "^2.0.8",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "glob": "^10.4.0"
  }
}
